<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/MatchMatch"
    android:background="@color/black20"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/ll_content"
        style="@style/MatchMatch"
        android:layout_marginBottom="@dimen/dimen220"
        android:layout_marginLeft="@dimen/dimen38"
        android:layout_marginRight="@dimen/dimen38"
        android:layout_marginTop="@dimen/dimen130"
        android:background="@drawable/shape_clearcache"
        android:gravity="center"
        android:orientation="vertical">

        <RelativeLayout style="@style/MatchWrap">

            <TextView
                style="@style/WrapWrap"
                android:layout_centerHorizontal="true"
                android:paddingBottom="@dimen/dimen24"
                android:paddingTop="@dimen/dimen24"
                android:text="@string/myVehicle"
                android:textColor="@color/textColor"
                android:textSize="@dimen/textsize19"
                android:textStyle="bold" />


            <ImageView
                android:id="@+id/img_cancel"
                android:layout_width="@dimen/dimen40"
                android:layout_height="@dimen/dimen40"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/dimen5"
                android:padding="@dimen/dimen10"
                android:src="@mipmap/logincancel" />

        </RelativeLayout>


        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen1"
            android:background="@color/background" />

        <ListView
            android:id="@+id/lv_myVehicle"
            style="@style/MatchWrap"
            android:divider="@color/background"
            android:dividerHeight="@dimen/dimen1"
            android:listSelector="@android:color/transparent"
            android:scrollbars="none"
            tools:listitem="@layout/item_myvehicle" />

    </LinearLayout>

    <TextView
        android:id="@+id/tv_determine"
        android:layout_width="@dimen/dimen195"
        android:layout_height="@dimen/dimen52"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="@dimen/dimen130"
        android:background="@drawable/shape_country"
        android:gravity="center"
        android:text="@string/determine"
        android:textColor="@color/whiteColors" />

</RelativeLayout>